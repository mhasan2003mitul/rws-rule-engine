<ruleService
        name="DakkapelService"
        xmlns="http://wso2.org/carbon/rules"
        targetNamespace="http://dakkapel.facts.re.rws.nl">
    <ruleSet>
        <rule resourceType="regular" sourceType="file">dakkapelDefinitieRules.drl</rule>
    </ruleSet>
    <operation name="definitieRuleService">
        <input wrapperElementName="definitieRuleRequest" targetNamespace="http://dakkapelex.facts.re.rws.nl">
            <fact type="nl.rws.re.facts.dakkapelx.Grondslag" targetNamespace="http://dakkapelex.facts.re.rws.nl"></fact>
            <fact type="nl.rws.re.facts.dakkapelx.Vraag" targetNamespace="http://dakkapelex.facts.re.rws.nl"></fact>
        </input>
        <output wrapperElementName="definitieRuleResponse" targetNamespace="http://dakkapel.facts.re.rws.nl">
            <fact type="nl.rws.re.facts.dakkapelx.GebruikerVragen" targetNamespace="http://dakkapelex.facts.re.rws.nl"></fact>
            <fact type="nl.rws.re.facts.dakkapelx.RuleResultaat" targetNamespace="http://dakkapelex.facts.re.rws.nl"></fact>
            <fact type="nl.rws.re.facts.error.Error" targetNamespace="http://dakkapelex.facts.re.rws.nl"></fact>
        </output>
    </operation>
</ruleService>
