<ruleService
        name="DakkapelService"
        xmlns="http://wso2.org/carbon/rules"
        targetNamespace="http://dakkapel.facts.re.rws.nl">
    <ruleSet>
        <rule resourceType="regular" sourceType="file">dakkapel.drl</rule>
    </ruleSet>
    <operation name="dakkapelService">
        <input wrapperElementName="dakkapelRequest" targetNamespace="http://dakkapel.facts.re.rws.nl">
            <fact type="nl.rws.re.facts.dakkapel.Step" targetNamespace="http://dakkapel.facts.re.rws.nl"></fact>
            <fact type="nl.rws.re.facts.dakkapel.Grondslag" targetNamespace="http://dakkapel.facts.re.rws.nl"></fact>
            <fact type="nl.rws.re.facts.dakkapel.Vraag" targetNamespace="http://dakkapel.facts.re.rws.nl"></fact>
            <fact type="java.util.ArrayList"></fact>
        </input>
        <output wrapperElementName="dakkapelResponse" targetNamespace="http://dakkapel.facts.re.rws.nl">
            <fact type="nl.rws.re.facts.dakkapel.GebruikerVragen" targetNamespace="http://dakkapel.facts.re.rws.nl"></fact>
        </output>
    </operation>
</ruleService>
